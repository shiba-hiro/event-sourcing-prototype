{
  "name": "event-gateway",
  "version": "0.0.1",
  "main": "build/Server.js",
  "scripts": {
    "dev": "PROJECT_ID=`gcloud config get-value project --configuration event-source` KEY_FILENAME=gcloud-configs/event-gateway-iam.json ts-node ./src/Server.ts",
    "build": "tsc --outDir build/",
    "start": "node ./build/Server.js",
    "lint": "eslint --parser @typescript-eslint/parser --ext .ts .",
    "typecheck": "tsc --noEmit",
    "test": "yarn lint && yarn typecheck",
    "precommit": "yarn test"
  },
  "author": "Hirofumi Shibata",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^2.4.0",
    "@types/express": "^4.17.1",
    "@types/uuid": "^3.4.5",
    "express": "^4.17.1",
    "log4js": "^5.2.2",
    "typescript": "^3.6.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "ts-node": "^8.4.1"
  }
}
